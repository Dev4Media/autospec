{
    "name": "d4m/autospec",
    "type": "library",
    "description": "Autospec Continuous Testing tool",
    "keywords": ["test", "spec", "bdd"],
    "license": "MIT",
    "homepage": "http://www.dev4media.pe",
    "authors":  [
        {
            "name":     "Dev4Media",
            "homepage": "http://www.dev4media.pe"
        },
        {
            "name":     "Raul Rodriguez",
            "email":    "raulrodriguez782@gmail.com"

        }
    ],

    "require":      {
        "php":  ">=5.3.3",
        "symfony/console":  "~2.1"
    },

    "require-dev":  {
        "phpspec/phpspec": "2.0.*@dev",
        "henrikbjorn/lurker": "1.0.*@dev"
    },

    "autoload":     {
        "psr-0":    {   "D4m\\AutoSpecExtension":   "src/"  }
    },

    "extra":    {
        "branch-alias": {
            "dev-master":   "1.0.x-dev"
        }
    },

    "config": {
        "bin-dir": "bin"
    },

    "bin": ["bin/phpspec"]
}